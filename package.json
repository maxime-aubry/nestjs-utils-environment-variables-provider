{
  "name": "nestjs-environment-variables-provider-monorepo",
  "version": "1.0.1",
  "description": "nestjs-environment-variables-provider monorepo",
  "type": "module",
  "workspaces": [
    "demo",
    "packages/*",
    "tests"
  ],
  "scripts": {
    "build": "npm run build -w packages/nestjs-environment-variables-provider",
    "format": "npm run format -w packages/nestjs-environment-variables-provider",
    "lint": "npm run lint -w packages/nestjs-environment-variables-provider",
    "clean-code": "npm run clean-code --workspaces",
    "test": "npm run test -w tests",
    "docs": "npm run docs -w packages/nestjs-environment-variables-provider",
    "postinstall": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
